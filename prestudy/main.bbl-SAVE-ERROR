      \strng{namehash}{6edd9fa176cb9288e5e7364635837c45}
      \strng{fullhash}{ff268e792a3d9decd858e190a88ad968}
      \strng{fullhashraw}{ff268e792a3d9decd858e190a88ad968}
      \strng{bibnamehash}{ff268e792a3d9decd858e190a88ad968}
